{
  "presets": [
    ["@babel/preset-env", {
      "debug": false,
      "useBuiltIns": false
    }],
    "@babel/preset-react"
  ],
  "plugins": [
    "syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    [ "@babel/plugin-proposal-class-properties", { "loose": true } ],
    "transform-async-to-generator"
  ]
}
